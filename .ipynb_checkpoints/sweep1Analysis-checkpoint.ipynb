    "   \"source\": [\n",
    "    \"# Sweep Results Analysis\\n\",\n",
    "    \"Basic analysis of sweep1.csv.\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"import pandas as pd\\n\",\n",
    "    \"import matplotlib.pyplot as plt\\n\",\n",
    "    \"import seaborn as sns\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Load the CSV\\n\",\n",
    "    \"df = pd.read_csv('sweep1.csv')\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Show the first few rows\\n\",\n",
    "    \"df.head()\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# Show summary statistics\\n\",\n",
    "    \"df.describe()\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# Plot distribution of finalMSE\\n\",\n",
    "    \"plt.figure(figsize=(8, 4))\\n\",\n",
    "    \"sns.histplot(df['finalMSE'], bins=30, kde=True)\\n\",\n",
    "    \"plt.title('Distribution of finalMSE')\\n\",\n",
    "    \"plt.xlabel('finalMSE')\\n\",\n",
    "    \"plt.ylabel('Count')\\n\",\n",
    "    \"plt.show()\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# Plot scatter of finalMSE vs. totalWells\\n\",\n",
    "    \"plt.figure(figsize=(8, 4))\\n\",\n",
    "    \"sns.scatterplot(x='totalWells', y='finalMSE', data=df)\\n\",\n",
    "    \"plt.title('finalMSE vs. totalWells')\\n\",\n",
    "    \"plt.xlabel('Total Wells')\\n\",\n",
    "    \"plt.ylabel('finalMSE')\\n\",\n",
    "    \"plt.show()\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# Show the best runs (lowest finalMSE)\\n\",\n",
    "    \"df.nsmallest(10, 'finalMSE')\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# Correlation heatmap\\n\",\n",
    "    \"plt.figure(figsize=(10, 6))\\n\",\n",
    "    \"sns.heatmap(df.corr(numeric_only=True), annot=True, cmap='coolwarm')\\n\",\n",
    "    \"plt.title('Correlation Heatmap')\\n\",\n",
    "    \"plt.show()\"\n",
    "   ]\n",
